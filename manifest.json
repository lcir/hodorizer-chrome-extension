{
  "manifest_version": 2,
  "name": "Hodorizer",
  "description": "This extension translate all to Hodor language.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": ["tabs", "http://*/*", "https://*/*"]
}